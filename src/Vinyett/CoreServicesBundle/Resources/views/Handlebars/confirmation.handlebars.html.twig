{% extends "CoreServicesBundle:Handlebars:base.html.twig" %}

{% block handlebars_body %}
{% raw %}
<div class="organizer_modal_header">
    {{title}}
</div>

<div class="organizer_modal_content">
    {{content}}
</div>

<div class="organizer_modal_basebar">
    <button type="submit" class="button" onclick="{{buttons.submit.onclick}}">{{buttons.submit.text}}</button>
    <button type="cancel" class="button" onclick="{{buttons.cancel.onclick}}">{{buttons.cancel.text}}</button>
</div>
{% endraw %}
{% endblock %}